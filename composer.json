{
    "name": "maxserv/fal_s3",
    "description": "Connect FAL/TYPO3 to any of the configured S3 buckets with a few clicks. File based configuration allows specific buckets depending on the context of your application.",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0",
    "keywords": [
        "TYPO3 CMS",
        "FAL",
        "S3"
    ],
    "homepage": "https://www.maxserv.com",
    "authors": [
        {
            "name": "Arno Schoon",
            "email": "arno@maxserv.com",
            "role": "Lead Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "MaxServ\\FalS3\\": "Classes/"
        }
    },
    "replace": {
        "typo3-ter/fal_s3": "self.version"
    },
    "require": {
        "aws/aws-sdk-php": "^3.2.3",
        "typo3/cms-backend": ">=6.2.12, <10.4.99",
        "typo3/cms-core": ">=6.2.12, <10.4.99",
        "typo3/cms-extbase": ">=6.2.12, <10.4.99"
    },
    "require-dev": {
        "nimut/testing-framework": "^4.1 || ^5.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "fal_s3"
        }
    }
}
